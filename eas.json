{
  "cli": {
    "version": ">= 16.12.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "node": "18.x.x",
      "image": "latest"
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "NPM_CONFIG_LEGACY_PEER_DEPS": "true"
      },
      "node": "18.x.x",
      "image": "latest"
    },
    "production": {
      "autoIncrement": true,
      "node": "18.x.x",
      "image": "latest"
    }
  },
  "submit": {
    "production": {}
  }
}
